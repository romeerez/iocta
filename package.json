{
  "name": "iocta",
  "type": "module",
  "version": "0.0.3",
  "description": "Simple Dependency Injection containers for TypeScript",
  "repository": "https://github.com/romeerez/iocta",
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "test": "vitest",
    "types": "tsc",
    "build": "tsdown build src/index.ts --format esm,cjs --sourcemap && rm dist/index.d.cts",
    "lint": "eslint . --fix",
    "format": "prettier . --write"
  },
  "keywords": [
    "dependency injection",
    "dependency",
    "injection",
    "di",
    "ioc",
    "container",
    "typescript"
  ],
  "author": "Roman Kushyn",
  "license": "MIT",
  "packageManager": "pnpm@10.12.1",
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/node": "^24.9.2",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
    "tsdown": "^0.15.12",
    "typescript": "^5.9.3",
    "vitest": "^4.0.5"
  }
}
